{ "useGPU": false,
  "use64bit": true,
  "debug_nans": true,
  "enable_jit": true,
  "seed": 3264,
  "action_dim": 1,
  "n_iters": 50,
  "models": {
    "id": "sum_of_half_spaces",
    "params": {
      "n_summands": 10,
      "instance_idx": 0,
      "specs": {
        "hmc_model": {
          "is_relaxed": true,
          "compiler_kwargs": {
            "n_rollouts": 1,
            "weight": 5.0
          }
        },
        "train_model": {
          "is_relaxed": false,
          "compiler_kwargs": {
            "n_rollouts": 128
          }
        },
        "eval_model": {
          "is_relaxed": false,
          "compiler_kwargs": {
            "n_rollouts": 32
          }
        }
      }
    }
  },
  "bijector": {
    "id": "identity",
    "params": {}
  },
  "policy": {
    "id": "multivar_normal_with_linear_parametrization",
    "params": {}
  },
  "optimizer": {
    "id": "adagrad",
    "params": {
      "learning_rate": 0.7
    }
  },
  "algorithm": {
    "id": "impsmp",
    "params": {
      "batch_size": 1024,
      "eval_batch_size": 32,
      "epsilon": 0.0,
      "est_Z": true,
      "hmc" : {
        "num_burnin_iters_per_chain": 512,
        "init_step_size": 0.003,
        "num_leapfrog_steps": 50,
        "init_distribution": {
          "type": "normal",
          "mean": 0.0,
          "var": 5.0
        },
        "reinit_strategy": "random_sample"
      },
      "verbose": true
    }
  },
  "save_to": "/tmp"
}
